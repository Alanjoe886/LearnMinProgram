<!--pages/home/home.wxml-->
<!-- 1.小程序的数据绑定：{{}} -> Mustache -->
<view>Hello {{name}}</view>
<view>我的年龄：{{age}}</view>

<!-- 2.列表展示 wx:for -->
<view wx:for="{{students}}">{{item.name}}的年龄：{{item.age}}</view>

<!-- 3.事件监听改变data -->
<view>当前计数：{{count}}</view>
<button size="mini" bindtap="add">+</button>
<button size="mini" bindtap="sub">-</button>
